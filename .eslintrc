{
  "plugins": [
    "mocha"
  ],
  "extends": "airbnb",
  "rules": {
    "indent": [2, "tab"],
    "no-tabs": 0,
    "import/no-extraneous-dependencies": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "no-console": 0
  },
  "globals": {
    "atom": true,
    "describe": true,
    "it": true,
    "context": true
  }
}
