{
  "name": "react-function-to-class",
  "main": "./dist/bundle",
  "version": "0.2.3",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "test": "mocha --compilers js:babel-core/register --source-maps --require babel-polyfill --require ./test-helper.js 'lib/**/*.test.js'",
    "test:watch": "npm run test -- --watch",
    "test:debug": "npm run test -- --inspect --debug-brk"
  },
  "activationEvents": [
    "react-function-to-class:toClass"
  ],
  "keywords": [
    "refactor",
    "react"
  ],
  "description": "Convert functions to React classes in a keystroke",
  "repository": "https://github.com/germtb/react-function-to-class",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "@babel/traverse": "^7.0.0-beta.40",
    "babylon": "^6.18.0"
  },
  "devDependencies": {
    "atom": "~1.0.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-mocha": "^4.9.0",
    "mocha": "^3.2.0",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-bundle-size": "^1.0.1",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-sizes": "^0.4.2",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0"
  }
}
